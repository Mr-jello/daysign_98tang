ENABLE_HEADLESS=true
# 重试间隔（分钟）
RETRY_INTERVAL=30
# 定时任务配置（默认每天凌晨0点20分执行）
CRON_SCHEDULE=0 20 0 * * *
# 立刻执行一次
RUN_ON_START=true
